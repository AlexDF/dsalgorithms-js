{
  "name": "dsalgorithms-js",
  "version": "0.1.0-alpha.1",
  "author": "Alexandre De Freitas",
  "description": "A Javascript library of data structures and algorithms.",
  "keywords": [
    "data-structures",
    "algorithms",
    "javascript"
  ],
  "scripts": {
    "dev": "vite --config vite.config.js",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "build": "npm run typecheck && rolldown -c",
    "lint:check": "eslint src test examples",
    "lint:fix": "eslint --fix src test examples",
    "format:check": "prettier --check src test",
    "format:fix": "prettier --write src test",
    "lint-staged": "lint-staged",
    "docs:build": "typedoc",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "test/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "homepage": "https://github.com/AlexDF/dsalgorithms-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexDF/dsalgorithms-js.git"
  },
  "bugs": {
    "url": "https://github.com/AlexDF/dsalgorithms-js/issues"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "imports": {
    "#config/*": "./config/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/dsalgorithms-js.min.umd.js",
      "import": "./dist/dsalgorithms-js.mjs",
      "require": "./dist/dsalgorithms-js.cjs"
    }
  },
  "unpkg": "./dist/dsalgorithms-js.min.umd.js",
  "jsdelivr": "./dist/dsalgorithms-js.min.umd.js",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.8.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@html-eslint/eslint-plugin": "^0.45.0",
    "@html-eslint/parser": "^0.45.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "3.6.2",
    "rolldown": "^1.0.0-beta.31",
    "rolldown-plugin-dts": "^0.15.6",
    "typedoc": "^0.28.10",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  }
}
